
public class metodos {

	public int metodo(boolean a, boolean b) {
		int ret = 0;
		if (a && b) {
			ret = 1;
		} else {
			ret = 2;
		}
		return ret;
	}

	int contar_letra(String cadena, String letra) {
		int contador, n, longitud;
		n = 0;
		contador = 0;
		longitud = cadena.length();
		if (longitud > 0) {
			do {
				if (cadena.substring(contador, contador+1).equals(letra)) {
					n++;
				}
				contador++;
				longitud--;
			} while (longitud > 0);
		}
		return n;
	}
	
	int cod_Area(){
	
	
	
	}
	
}
