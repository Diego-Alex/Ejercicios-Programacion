import static org.junit.Assert.*;

import org.junit.Test;

public class metodosTest {

	@Test
	public void testMetodo() {
		metodos prueba1=new metodos();
		boolean a,b,c;
		a=true;
		b=true;
		c=true;
		prueba1.metodo(a, b, c);
	}

	@Test
	public void testContar_letra() {
		metodos prueba2=new metodos();
		String cadena, letra;
		cadena="";
		letra="a";
		prueba2.contar_letra(cadena, letra);
	}

}
